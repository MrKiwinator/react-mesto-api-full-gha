{
    "env": {
        // "browser": true,
        // "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id"
          ]
        }
      ],
      // if this setting will be switched off
      // eslint start showing errors when console used
      "no-console": [
        "error",
        {
          "allow": [
            "log",
            "warn",
            "error"
          ]
        }
      ]
    }
}
